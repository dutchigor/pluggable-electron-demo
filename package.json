{
  "name": "pluggable-electron-demo",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "dev-vue": "vite",
    "dev-electron": "wait-on http://localhost:3000/index.html && electron .",
    "dev": "concurrently --kill-others npm:dev-vue npm:dev-electron",
    "start": "vite build && electron .",
    "pkg-plugin": "cd demo-plugin && npm run package"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/compiler-sfc": "^3.2.40",
    "concurrently": "^7.4.0",
    "electron": "^21.1.0",
    "vite": "^3.1.7",
    "vite-plugin-externals": "^0.5.1",
    "wait-on": "^6.0.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.2",
    "pluggable-electron": "^0.5.1",
    "vue": "^3.2.40"
  }
}